{
  "name": "@trinsic/web-ui",
  "version": "0.2.7",
  "description": "Trinsic's Web UI SDK to easily launch your verifications and capture the result",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --sourcemap --format=esm --outdir=dist --target=chrome58,firefox57,safari11,edge16",
    "postbuild": "tsc src/index.ts --declaration --emitDeclarationOnly --lib es2015,dom --outDir dist"
  },
  "types": "dist/index.d.ts",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "0.23.0",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "micromodal": "0.4.10"
  }
}
