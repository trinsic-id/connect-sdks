{
  "name": "@trinsic/expo-ui",
  "version": "0.1.0",
  "description": "Helper methods to launch and capture the results of a Trinsic verification",
  "main": "index.js",
  "scripts": {
    "build-js": "esbuild index.ts --bundle --platform=node --format=cjs --outdir=dist --target=es2020 --external:react --external:react-native --external:react/jsx-runtime --external:expo-web-browser --external:expo-modules-core",
    "build-ts": "tsc index.ts --declaration --emitDeclarationOnly --lib es6 --types node --outDir dist",
    "build": "npm run build-js && npm run build-ts"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.js",
  "author": "Trinsic",
  "homepage": "https://trinsic.id",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "0.23.0",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "expo-web-browser": "^13.0.3"
  }
}
