{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "branchPrefix": "mewmba/renovate/",
  "gitAuthor": "mewmba Renovate Bot <mewmba@trinsic.id>",
  "repositories": ["trinsic-id/sdk"],
  "prHourlyLimit": 0,
  "includePaths": [
    ".github/renovate.json",
    "api-csharp/samples/Sample/**",
    "api-go/samples/**",
    "api-java/samples/**",
    "api-php/samples/**",
    "api-python/samples/**",
    "api-php/samples/**",
    "api-ruby/samples/**",
    "api-typescript/samples/**",
    "ui-web/samples/**"
  ],
  "ignoreDeps": [
    "vite",
    "express",
    "@types/node",
    "nodemon",
    "ts-node",
    "ts-node-dev",
    "typescript",
    "dotenv",
    "httparty",
    "sinatra",
    "sinatra-contrib",
    "rack",
    "rack-test",
    "minitest",
    "slim/slim",
    "slim/psr7",
    "vlucas/phpdotenv",
    "io.javalin:javalin",
    "org.slf4j:slf4j-simple",
    "io.github.cdimascio:dotenv-java",
    "github.com/gofiber/fiber/v2",
    "github.com/joho/godotenv",
    "DotNetEnv"
  ],
  "packageRules": [
    {
      "groupName": "all-dependencies",
      "groupSlug": "all-dependencies",
      "separateMinorPatch": false,
      "matchUpdateTypes": ["minor", "patch", "major"]
    }
  ]
}
